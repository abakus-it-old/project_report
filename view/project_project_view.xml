<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
        <record model="ir.ui.view" id="edit_project_report">
           <field name="name">edit_project_report</field>
            <field name="model">project.project</field>
            <field name="inherit_id" ref="project.edit_project"/>
            <field name="arch" type="xml">
                <group name="group_alias" position="after">
                    <group string="Project report">
                        <button name="print_report" type="object" string="Print the Report"/>
                    </group>
                </group>
            </field>
        </record>
    </data>
</openerp>



