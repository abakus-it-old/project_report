<?xml version="1.0"?>
<openerp>
    <data>
        <report 
			id="project_abakus"
			string="Project Report"
			model="project.project"
			report_type="qweb-pdf"
			file="project_report"
			name="project_report.report_project"
            attachment_use="True"
            attachment='"ProjectReport-"+object.name.replace(" ","").replace("*","")+"("+time.strftime("%Y%m%d_%H%M%S")+").pdf"'
		/>

        <template id="report_project_document">
            <t t-name="contract_report.report_contract_document">
                <t t-call="report.external_layout">
                    <div>TEST</div>
                </t>
            </t>
        </template>

        <template id="report_project">
            <t t-call="report.html_container">
                <t t-foreach="doc_ids" t-as="doc_id">
                    <t t-raw="translate_doc(doc_id, doc_model, 'partner_id.lang', 'project_report.report_project_document')"/>
                </t>
            </t>
        </template>
    </data>
</openerp>
